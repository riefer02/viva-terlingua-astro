    "@radix-ui/react-slot": "^1.1.1",
    "@strapi/sdk-js": "1.0.0-beta.0",
    "@tailwindcss/vite": "^4.0.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "strapi-sdk-js": "^2.3.3",
    "tailwind-merge": "^2.6.0",
